    <%= isLoaderGeneratable(field) ? '' : '// ' %>  const loadedModels = <%- getTargetLoadedModels(field) %>;
    <%= isLoaderGeneratable(field) ? '' : '// ' %>  return <%= getOtherEntityStrings(field).entityClass %>.fromArray(loadedModels);
    <%= isLoaderGeneratable(field) ? '' : '// ' %>},
